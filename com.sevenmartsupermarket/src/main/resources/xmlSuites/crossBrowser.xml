<suite name="CrossBrowserExecution" thread_count="10" parallel="tests">
	<listeners>
		<listener class-name="com.sevenmartsupermarket.listeners.ExtentReporter"></listener>
	</listeners>
	<test name="CrossBrowserExecutionInChrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.sevenmartsupermarket.tests.LoginTest"></class>
		</classes>
	</test>
	<test name="CrossBrowserExecutionInEdge">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="com.sevenmartsupermarket.tests.LoginTest"></class>
		</classes>
	</test>
	<test name="CrossBrowserExecutionInFirefox">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.sevenmartsupermarket.tests.LoginTest"></class>
		</classes>
	</test>


</suite>